{
	"compilerOptions": {
		"target": "es2016",
		"module": "esnext",
		"moduleResolution": "bundler",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"baseUrl": ".",
		"paths": {
			"@realityspiral/plugin-instrumentation": [
				"../plugin-instrumentation/src"
			],
			"@realityspiral/plugin-coinbase": ["../plugin-coinbase/src"],
			"@realityspiral/plugin-rofl": ["../plugin-rofl/src"]
		}
	},
	"include": ["src"]
}
