		}
	},
	"files": ["dist"],
	"dependencies": {
		"@elizaos/core": "^0.1.9",
    "@realityspiral/sentry": "workspace:*",
		"@realityspiral/plugin-github": "workspace:*",
		"@realityspiral/plugin-instrumentation": "workspace:*",
		"@sentry/node": "^7.101.1",
		"@octokit/rest": "20.1.1",
		"@octokit/types": "12.6.0",
		"glob": "10.4.5",
		"simple-git": "3.27.0",
		"zod": "3.23.8"
	},
	"devDependencies": {
		"@types/glob": "8.1.0",
		"tsup": "8.3.5",
		"vitest": "^1.2.1"
